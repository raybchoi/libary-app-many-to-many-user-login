
<!-- Need to specify the url to be "/sessions" because without that because it is a Users.new it routes to "/user" -->
<hr>
  <%= form_for @user , url:"/sessions", method: "post" do |f| %>
    <div>
      <%= f.email_field :email, placeholder: "Email" %>
    </div>
    <div>
      <%= f.password_field :password, placeholder: "Password" %>
    </div>
  <%= f.submit "Log In" %>
  <% end %>
</hr>
